<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat code coverage">
				<Configuration>
					<Format>cobertura</Format>
					<ExcludeByFile>**/Migrations/**/*.cs</ExcludeByFile>
					<Exclude>[BarSystem]*Migrations*,[BarSystem]Program,[BarSystem]AutoGeneratedProgram</Exclude>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>